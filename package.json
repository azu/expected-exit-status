    "test:2": "./bin/cmd.mjs 2 --command \"exit 2\"",
    "test:example": "./bin/cmd.mjs 0 --command \"ls\""
  },
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "meow": "^10.1.2"
  }
